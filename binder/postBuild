python -m bash_kernel.install
go get github.com/ericchiang/pup
go get -u github.com/tomnomnom/gron
mkdir -p /home/jovyan/local/bin
npm install mapshaper --prefix /home/jovyan/local/bin
cd /home/jovyan/local/bin
ln -s /home/jovyan/local/bin/node_modules/mapshaper/bin/mapshaper mapshaper
ln -s /home/jovyan/go/bin/pup pup

# sqlite e spatialite
curl -sL "https://www.sqlite.org/2018/sqlite-autoconf-3230100.tar.gz" > /home/jovyan/local/sqlite-autoconf-3230100.tar.gz
tar -xzf ./sqlite-autoconf-3230100.tar.gz
cd /home/jovyan/local/sqlite-autoconf-3230100
./configure
make install
cd /home/jovyan/local/
wget http://www.gaia-gis.it/gaia-sins/libspatialite-sources/libspatialite-4.3.0.zip
unzip -qq ./libspatialite-4.3.0.zip
cd libspatialite-4.3.0
export PKG_CONFIG_PATH=/usr/local/lib/pkgconfig
./configure —enable-freexl=no
sudo make install
cd /home/jovyan/local/
wget http://www.gaia-gis.it/gaia-sins/spatialite-tools-sources/spatialite-tools-4.3.0.tar.gz
tar -xzf ./spatialite-tools-4.3.0.tar.gz
cd spatialite-tools-4.3.0
./configure —enable-freexl=no
make install